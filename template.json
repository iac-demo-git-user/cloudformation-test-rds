{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Description" : "A sample template",
    "Resources": {
        "RestApi": {
            "Type": "AWS::ApiGateway::RestApi",
            "Properties": {
                "Name": "myApi",
                "Parameters": {
                    "endpointConfigurationTypes": "PRIVATE"
                }
            }
        },
        
        "MockMethod": {
        "Type": "AWS::ApiGateway::Method",
        "Properties": {
            "RestApiId": {
                "Ref": "RestApi"
            },
            "ResourceId": {
                "Fn::GetAtt": [
                    "RestApi",
                    "RootResourceId"
                ]
            },
            "HttpMethod": "GET",
            "AuthorizationType": "NONE",
            "Integration": {
                "Type": "MOCK"
            }
        }
    }
  }
}
